
@{
    ViewBag.Title = "Index";
}

<div class="jumbotron">
    <h1 class="display-4">Register</h1>
</div>
<br><br>
<div class="row">
    <div class="col-md-4">
    </div>
    <div class="col-md-4">
        <form method="post" action="api/ParkClient?">
            <div class="form-group">
                <label for="licenseNumber">Licenseplate number</label>
                <input type="text" class="form-control" name="licenseNumber" placeholder="XX12345">
            </div>
            <div class="form-group">
                <label for="companyParkingCode">Company parking code</label>
                <input type="text" class="form-control" name="companyParkingCode" placeholder="A1B2C">
            </div>
            <div class="form-group">
                <input type="submit" class="btn-lg btn-primary btn-block" value="Submit">
            </div>
        </form>
    </div>
    <div class="col-md-4">
    </div>
</div>
